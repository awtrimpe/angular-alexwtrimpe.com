{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write src/**/*.{graphql,json,md,ts,yaml}",
    "beautify": "js-beautify --end-with-newline --indent-size=1 src/**/*.{html,css,js}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.11",
    "@angular/http": "^7.2.16",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@nowzoo/ngx-highlight-js": "^2.1.15",
    "@nowzoo/sheetload": "^0.0.7",
    "core-js": "^3.6.5",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^8.1.3",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@types/puppeteer": "^1.20.3",
    "codelyzer": "^5.1.2",
    "husky": "^3.1.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "js-beautify": "^1.10.2",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "protractor": "^7.0.0",
    "puppeteer": "^1.20.0",
    "rxjs-tslint": "^0.1.8",
    "ts-node": "~7.0.0",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3",
    "webpack": "^4.43.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{graphql,json,md,ts,yaml}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{html,css,js}": [
      "js-beautify --end-with-newline --indent-size=2",
      "git add"
    ]
  }
}
